{
  "name": "cognicity-cards",
  "description": "Client input cards for crowd sourced disaster mapping applications.",
  "version": "1.0.3",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbanriskmap/cognicity-cards"
  },
  "scripts": {
    "start": "rm -rf src/routes/_route_handlers/* && au run --env dev --dep $DEP --watch",
    "test": "au build",
    "build": "rm -rf src/routes/_route_handlers/* scripts/* && au build --env prod --dep $DEP",
    "build-stage": "rm -rf src/routes/_route_handlers/* scripts/* && au build --env stage --dep $DEP",
    "build-dev": "rm -rf src/routes/_route_handlers/* scripts/* && au build --env dev --dep $DEP",
    "docs": "node_modules/.bin/esdoc"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./esdoc/",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "decorators": true
        }
      }
    ],
    "option": {
      "lint": {
        "enable": true
      },
      "coverage": {
        "enable": true
      }
    }
  },
  "dependencies": {
    "aurelia-animator-css": "^1.0.2",
    "aurelia-bootstrapper": "^2.2.0",
    "aurelia-fetch-client": "^1.4.0",
    "aurelia-http-client": "^1.3.0",
    "aurelia-protractor-plugin": "^1.0.3",
    "bluebird": "^3.5.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-replace": "^0.6.1",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.1",
    "less": "^2.7.2",
    "nprogress": "^0.2.0",
    "requirejs": "^2.3.3",
    "text": "github:requirejs/text#latest",
    "whatwg-fetch": "^2.0.4"
  },
  "peerDependencies": {},
  "devDependencies": {
    "aurelia-cli": "^0.29.0",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "aurelia-tools": "^1.0.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.24.5",
    "canned": "^0.3.12",
    "connect-history-api-fallback": "^1.5.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.3",
    "gulp-changed-in-place": "^2.2.0",
    "gulp-cli": "^1.3.0",
    "gulp-eslint": "^3.0.1",
    "gulp-flatten": "^0.3.1",
    "gulp-less": "^3.5.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.3.0",
    "gulp-sourcemaps": "^2.6.4",
    "jasmine-core": "^2.99.1",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "minimatch": "^3.0.4",
    "notifyjs-browser": "^0.4.2",
    "through2": "^2.0.3",
    "topojson-client": "^3.0.0",
    "uglify-js": "~2.7.5",
    "vinyl-fs": "^2.4.4"
  }
}
